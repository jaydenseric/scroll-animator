{
  "name": "scroll-animator",
  "version": "0.1.0-alpha.5",
  "description": "A collection of functions for animating browser scroll.",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "http://jaydenseric.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://jaydenseric@github.com/jaydenseric/scroll-animator.git"
  },
  "homepage": "https://github.com/jaydenseric/scroll-animator#readme",
  "bugs": "https://github.com/jaydenseric/scroll-animator/issues",
  "keywords": [
    "scroll",
    "scrolling",
    "animator",
    "animate",
    "es6"
  ],
  "main": "dist/scroll-animator.js",
  "jsnext:main": "dist/scroll-animator.mjs",
  "scripts": {
    "lint": "eslint .",
    "prebuild": "npm run lint",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "docs": "jsdoc2md -t readme-template.hbs lib/**/*.js > readme.md",
    "prepublish": "npm run build && npm run docs"
  },
  "dependencies": {
    "get-scrollmax-x": "^0.2.0",
    "get-scrollmax-y": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-preset-es2015-rollup": "^1.1.1",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-standard": "^1.3.2",
    "jsdoc-to-markdown": "^1.3.6",
    "rollup": "^0.34.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-watch": "^2.5.0"
  },
  "babel": {
    "presets": [
      "es2015-rollup"
    ]
  },
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "valid-jsdoc": [
        2,
        {
          "requireReturn": false,
          "prefer": {
            "return": "returns"
          }
        }
      ]
    }
  }
}
