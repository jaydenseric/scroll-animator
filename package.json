{
  "name": "scroll-animator",
  "version": "4.0.0",
  "description": "Smart, lightweight functions to animate browser scroll.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/scroll-animator",
  "homepage": "https://github.com/jaydenseric/scroll-animator#readme",
  "bugs": "https://github.com/jaydenseric/scroll-animator/issues",
  "funding": "https://github.com/sponsors/jaydenseric",
  "keywords": [
    "scroll",
    "scrolling",
    "animator",
    "animate",
    "esm",
    "mjs"
  ],
  "files": [
    "private",
    "public"
  ],
  "sideEffects": false,
  "main": "public",
  "exports": {
    ".": {
      "import": "./public/index.mjs",
      "require": "./public/index.js"
    },
    "./public/*": "./public/*",
    "./package": "./package.json",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >= 16.0.0"
  },
  "browserslist": "Node 12.22 - 13 and Node < 13, Node 14.17 - 15 and Node < 15, Node >= 16, > 0.5%, not OperaMini all, not IE > 0, not dead",
  "devDependencies": {
    "coverage-node": "^6.0.1",
    "esbuild": "^0.14.17",
    "eslint": "^8.8.0",
    "eslint-config-env": "^23.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gzip-size": "^7.0.0",
    "jsdoc-md": "^11.0.2",
    "prettier": "^2.5.1",
    "test-director": "^8.0.1"
  },
  "scripts": {
    "jsdoc": "jsdoc-md",
    "test": "npm run test:eslint && npm run test:prettier && npm run test:jsdoc && npm run test:api",
    "test:eslint": "eslint --ext mjs,js .",
    "test:prettier": "prettier -c .",
    "test:jsdoc": "jsdoc-md -c",
    "test:api": "coverage-node test/index.mjs",
    "prepublishOnly": "npm test"
  }
}
