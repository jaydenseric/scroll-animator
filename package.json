{
  "name": "scroll-animator",
  "version": "1.1.0",
  "description": "A collection of functions for animating browser scroll.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/scroll-animator",
  "homepage": "https://github.com/jaydenseric/scroll-animator#readme",
  "bugs": "https://github.com/jaydenseric/scroll-animator/issues",
  "keywords": [
    "scroll",
    "scrolling",
    "animator",
    "animate",
    "esm",
    "mjs"
  ],
  "files": [
    "lib"
  ],
  "main": "lib",
  "sideEffects": false,
  "engines": {
    "node": ">=6"
  },
  "browserslist": "> 0.5%, not dead, node 6",
  "dependencies": {
    "get-scrollmax-x": "^0.2.0",
    "get-scrollmax-y": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.9.0",
    "eslint-config-env": "^2.0.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-import-order-alphabetical": "^0.0.1",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.0",
    "jsdoc-md": "^1.7.0",
    "lint-staged": "^8.0.5",
    "prettier": "^1.15.2"
  },
  "scripts": {
    "prepare": "npm run prepare:clean && npm run prepare:mjs && npm run prepare:js && npm run prepare:jsdoc && npm run prepare:prettier",
    "prepare:clean": "rm -rf lib",
    "prepare:mjs": "BABEL_ESM=1 babel src -d lib --keep-file-extension",
    "prepare:js": "babel src -d lib",
    "prepare:jsdoc": "jsdoc-md",
    "prepare:prettier": "prettier 'lib/**/*.{mjs,js}' readme.md --write",
    "test": "npm run test:eslint && npm run test:prettier",
    "test:eslint": "eslint . --ext mjs,js",
    "test:prettier": "prettier '**/*.{json,yml,md}' -l",
    "prepublishOnly": "npm run prepare && npm test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{mjs,js}": "eslint",
    "*.{json,yml,md}": "prettier -l"
  },
  "eslintIgnore": [
    "lib"
  ],
  "eslintConfig": {
    "extends": [
      "env"
    ]
  },
  "prettier": {
    "proseWrap": "never",
    "singleQuote": true,
    "semi": false
  }
}
