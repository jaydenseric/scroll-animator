{
  "name": "scroll-animator",
  "version": "1.1.0",
  "description": "A collection of functions for animating browser scroll.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/scroll-animator",
  "homepage": "https://github.com/jaydenseric/scroll-animator#readme",
  "bugs": "https://github.com/jaydenseric/scroll-animator/issues",
  "keywords": [
    "scroll",
    "scrolling",
    "animator",
    "animate",
    "es6"
  ],
  "main": "lib/scroll-animator.js",
  "module": "lib/scroll-animator.module.js",
  "sideEffects": false,
  "scripts": {
    "prepare": "npm run prepare:clean && npm run prepare:build && npm run prepare:jsdoc",
    "prepare:clean": "rm -rf lib",
    "prepare:build": "rollup -c",
    "prepare:jsdoc":  "jsdoc2md -t readme-template.hbs src/**/*.js > readme.md",
    "lint": "eslint .",
    "prepublishOnly": "npm run prepare && npm run lint"
  },
  "dependencies": {
    "get-scrollmax-x": "^0.2.0",
    "get-scrollmax-y": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-preset-es2015-rollup": "^1.2.0",
    "eslint": "^3.7.0",
    "eslint-config-barebones": "^0.1.2",
    "jsdoc-to-markdown": "^1.3.7",
    "rollup": "^0.36.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-watch": "^2.5.0"
  },
  "babel": {
    "presets": [
      "es2015-rollup"
    ]
  },
  "eslintConfig": {
    "extends": "barebones"
  }
}
